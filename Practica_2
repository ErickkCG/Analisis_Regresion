
x<-c(132,129,120,113.2,105,92,84,83.2,88.4,59,80,81.5,71,69.2)
y<-c(46,48,51,52.1,54,52,59,58.7,61.6,64,61.4,54.6,58.8,58)
plot (x,y,xlab="X",ylab = "Y",main = "Ejercicio")

#--------------------------------------------------------------------

Sxx<-sum ((x-mean(x))^2)

Sxy<- sum((x-mean(x))*(y-mean(y)))

b1<-(Sxy/Sxx)
b0<-mean(y)-b1*mean(x)


#------SOLUCION-------
#Sxx=6802.77
#Sxy=-1424.42
#B0=75.2124319329198
#B1=-.20938741649054

#Recta ajusta
# y= 75.2124 -.209387x 


lm(y~x)
